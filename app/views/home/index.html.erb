<!-- Hero Section -->
<section class="relative min-h-[90vh] flex items-center overflow-hidden">
  <!-- Animated Background -->
  <div class="absolute inset-0 bg-linear-to-br from-base-100 via-base-200 to-base-300">
    <div class="absolute top-20 left-10 w-72 h-72 bg-primary/20 rounded-full blur-3xl animate-pulse"></div>
    <div class="absolute bottom-20 right-10 w-96 h-96 bg-secondary/20 rounded-full blur-3xl animate-pulse" style="animation-delay: 1s;"></div>
    <div class="absolute top-1/2 left-1/2 -translate-x-1/2 -translate-y-1/2 w-[600px] h-[600px] bg-accent/10 rounded-full blur-3xl"></div>
  </div>

  <!-- Grid Pattern Overlay -->
  <div class="absolute inset-0 opacity-[0.03]" style="background-image: linear-gradient(currentColor 1px, transparent 1px), linear-gradient(90deg, currentColor 1px, transparent 1px); background-size: 60px 60px;"></div>

  <div class="relative z-10 max-w-7xl mx-auto px-6 py-20 text-center">
    <!-- Badge -->
    <div class="inline-flex items-center gap-2 px-4 py-2 rounded-full bg-primary/10 border border-primary/20 mb-8 animate-fade-in">
      <span class="relative flex h-2 w-2">
        <span class="animate-ping absolute inline-flex h-full w-full rounded-full bg-primary opacity-75"></span>
        <span class="relative inline-flex rounded-full h-2 w-2 bg-primary"></span>
      </span>
      <span class="text-sm font-medium text-primary">Now in Public Beta</span>
    </div>

    <!-- Main Headline -->
    <h1 class="text-5xl md:text-7xl lg:text-8xl font-black tracking-tight mb-6 leading-[0.9]">
      <span class="block bg-linear-to-r from-base-content via-primary to-secondary bg-clip-text text-transparent">
        Build Faster.
      </span>
      <span class="block mt-2 bg-linear-to-r from-secondary via-accent to-primary bg-clip-text text-transparent">
        Ship Smarter.
      </span>
    </h1>

    <!-- Subheadline -->
    <p class="max-w-2xl mx-auto text-xl md:text-2xl text-base-content/70 mb-12 leading-relaxed">
      The production-ready Rails starter kit that lets you focus on what matters—
      <span class="text-primary font-semibold">building your product</span>, not boilerplate.
    </p>

    <!-- CTA Buttons -->
    <div class="flex flex-col sm:flex-row gap-4 justify-center items-center mb-16">
      <%= link_to sign_in_path, class: "btn btn-primary btn-lg px-8 gap-2 group shadow-lg shadow-primary/25 hover:shadow-xl hover:shadow-primary/30 transition-all duration-300" do %>
        Get Started Free
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 group-hover:translate-x-1 transition-transform" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7l5 5m0 0l-5 5m5-5H6" />
        </svg>
      <% end %>
      <a href="#features" class="btn btn-ghost btn-lg gap-2 group">
        <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M14.752 11.168l-3.197-2.132A1 1 0 0010 9.87v4.263a1 1 0 001.555.832l3.197-2.132a1 1 0 000-1.664z" />
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
        </svg>
        Watch Demo
      </a>
    </div>

    <!-- Social Proof -->
    <div class="flex flex-col items-center gap-4">
      <div class="flex -space-x-3">
        <% 5.times do |i| %>
          <div class="w-10 h-10 rounded-full ring-2 ring-base-100 bg-linear-to-br from-primary to-secondary flex items-center justify-center text-primary-content font-bold text-sm">
            <%= ('A'.ord + i).chr %>
          </div>
        <% end %>
        <div class="w-10 h-10 rounded-full ring-2 ring-base-100 bg-base-300 flex items-center justify-center text-base-content text-xs font-medium">
          +2k
        </div>
      </div>
      <p class="text-base-content/60 text-sm">
        <span class="font-semibold text-base-content">2,000+</span> developers already shipping
      </p>
    </div>
  </div>

  <!-- Scroll Indicator -->
  <a href="#features" class="absolute bottom-8 left-1/2 -translate-x-1/2 flex flex-col items-center gap-2 text-base-content/40 hover:text-primary transition-colors">
    <span class="text-xs uppercase tracking-widest">Scroll</span>
    <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 animate-bounce" fill="none" viewBox="0 0 24 24" stroke="currentColor">
      <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3" />
    </svg>
  </a>
</section>

<!-- Features Section -->
<section id="features" class="relative py-32 bg-base-200">
  <!-- Background decoration -->
  <div class="absolute inset-0 overflow-hidden">
    <div class="absolute -top-40 -right-40 w-80 h-80 bg-primary/5 rounded-full blur-3xl"></div>
    <div class="absolute -bottom-40 -left-40 w-80 h-80 bg-secondary/5 rounded-full blur-3xl"></div>
  </div>

  <div class="relative z-10 max-w-7xl mx-auto px-6">
    <!-- Section Header -->
    <div class="text-center mb-20">
      <span class="inline-block px-4 py-1.5 rounded-full bg-secondary/10 text-secondary text-sm font-semibold mb-4">
        Features
      </span>
      <h2 class="text-4xl md:text-5xl lg:text-6xl font-black mb-6">
        Everything you need to
        <span class="bg-linear-to-r from-primary to-secondary bg-clip-text text-transparent">launch fast</span>
      </h2>
      <p class="max-w-2xl mx-auto text-lg text-base-content/60">
        Stop reinventing the wheel. Start with a solid foundation that scales with your ambition.
      </p>
    </div>

    <!-- Features Grid -->
    <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
      <!-- Feature 1 -->
      <div class="group relative bg-base-100 rounded-2xl p-8 shadow-sm hover:shadow-xl transition-all duration-500 border border-base-300 hover:border-primary/30">
        <div class="absolute inset-0 bg-linear-to-br from-primary/5 to-transparent rounded-2xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
        <div class="relative">
          <div class="w-14 h-14 rounded-xl bg-primary/10 flex items-center justify-center mb-6 group-hover:scale-110 transition-transform">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-7 w-7 text-primary" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3">Passwordless Auth</h3>
          <p class="text-base-content/60 leading-relaxed">
            Magic link authentication out of the box. No passwords to manage, no security headaches.
          </p>
        </div>
      </div>

      <!-- Feature 2 -->
      <div class="group relative bg-base-100 rounded-2xl p-8 shadow-sm hover:shadow-xl transition-all duration-500 border border-base-300 hover:border-secondary/30">
        <div class="absolute inset-0 bg-linear-to-br from-secondary/5 to-transparent rounded-2xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
        <div class="relative">
          <div class="w-14 h-14 rounded-xl bg-secondary/10 flex items-center justify-center mb-6 group-hover:scale-110 transition-transform">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-7 w-7 text-secondary" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3">Multi-Tenancy</h3>
          <p class="text-base-content/60 leading-relaxed">
            URL-based multi-tenancy built in. Each account gets isolated data with zero configuration.
          </p>
        </div>
      </div>

      <!-- Feature 3 -->
      <div class="group relative bg-base-100 rounded-2xl p-8 shadow-sm hover:shadow-xl transition-all duration-500 border border-base-300 hover:border-accent/30">
        <div class="absolute inset-0 bg-linear-to-br from-accent/5 to-transparent rounded-2xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
        <div class="relative">
          <div class="w-14 h-14 rounded-xl bg-accent/10 flex items-center justify-center mb-6 group-hover:scale-110 transition-transform">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-7 w-7 text-accent" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 7v10c0 2.21 3.582 4 8 4s8-1.79 8-4V7M4 7c0 2.21 3.582 4 8 4s8-1.79 8-4M4 7c0-2.21 3.582-4 8-4s8 1.79 8 4m0 5c0 2.21-3.582 4-8 4s-8-1.79-8-4" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3">SQLite, Postgres, or MySQL</h3>
          <p class="text-base-content/60 leading-relaxed">
            Start simple with SQLite, scale to Postgres or MySQL when you need. Same codebase, zero friction.
          </p>
        </div>
      </div>

      <!-- Feature 4 -->
      <div class="group relative bg-base-100 rounded-2xl p-8 shadow-sm hover:shadow-xl transition-all duration-500 border border-base-300 hover:border-info/30">
        <div class="absolute inset-0 bg-linear-to-br from-info/5 to-transparent rounded-2xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
        <div class="relative">
          <div class="w-14 h-14 rounded-xl bg-info/10 flex items-center justify-center mb-6 group-hover:scale-110 transition-transform">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-7 w-7 text-info" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3">Solid Components</h3>
          <p class="text-base-content/60 leading-relaxed">
            Built-in Solid Queue (jobs), Solid Cable (realtime), and Solid Cache for robust background jobs, broadcasting, and caching—all database-backed and production-ready.
          </p>
        </div>
      </div>

      <!-- Feature 5 -->
      <div class="group relative bg-base-100 rounded-2xl p-8 shadow-sm hover:shadow-xl transition-all duration-500 border border-base-300 hover:border-success/30">
        <div class="absolute inset-0 bg-linear-to-br from-success/5 to-transparent rounded-2xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
        <div class="relative">
          <div class="w-14 h-14 rounded-xl bg-success/10 flex items-center justify-center mb-6 group-hover:scale-110 transition-transform">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-7 w-7 text-success" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M7 16a4 4 0 01-.88-7.903A5 5 0 1115.9 6L16 6a5 5 0 011 9.9M15 13l-3-3m0 0l-3 3m3-3v12" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3">Kamal Deploy</h3>
          <p class="text-base-content/60 leading-relaxed">
            Deploy to any VPS with Docker. Zero-downtime deployments configured and ready to go.
          </p>
        </div>
      </div>

      <!-- Feature 6 -->
      <div class="group relative bg-base-100 rounded-2xl p-8 shadow-sm hover:shadow-xl transition-all duration-500 border border-base-300 hover:border-warning/30">
        <div class="absolute inset-0 bg-linear-to-br from-warning/5 to-transparent rounded-2xl opacity-0 group-hover:opacity-100 transition-opacity"></div>
        <div class="relative">
          <div class="w-14 h-14 rounded-xl bg-warning/10 flex items-center justify-center mb-6 group-hover:scale-110 transition-transform">
            <svg xmlns="http://www.w3.org/2000/svg" class="h-7 w-7 text-warning" fill="none" viewBox="0 0 24 24" stroke="currentColor">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
            </svg>
          </div>
          <h3 class="text-xl font-bold mb-3">Security First</h3>
          <p class="text-base-content/60 leading-relaxed">
            Brakeman, bundler-audit, and RuboCop preconfigured. Stay secure from day one.
          </p>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- Testimonials Section -->
<section class="relative py-32 bg-base-100 overflow-hidden">
  <!-- Background decoration -->
  <div class="absolute inset-0">
    <div class="absolute top-1/2 left-1/2 -translate-x-1/2 -translate-y-1/2 w-[800px] h-[800px] bg-linear-to-r from-primary/5 via-transparent to-secondary/5 rounded-full blur-3xl"></div>
  </div>

  <div class="relative z-10 max-w-7xl mx-auto px-6">
    <!-- Section Header -->
    <div class="text-center mb-20">
      <span class="inline-block px-4 py-1.5 rounded-full bg-primary/10 text-primary text-sm font-semibold mb-4">
        Testimonials
      </span>
      <h2 class="text-4xl md:text-5xl lg:text-6xl font-black mb-6">
        Loved by
        <span class="bg-linear-to-r from-primary to-secondary bg-clip-text text-transparent">developers</span>
      </h2>
      <p class="max-w-2xl mx-auto text-lg text-base-content/60">
        Join thousands of developers who shipped their ideas faster with our starter kit.
      </p>
    </div>

    <!-- Testimonials Grid -->
    <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
      <!-- Testimonial 1 -->
      <div class="group relative">
        <div class="absolute inset-0 bg-linear-to-r from-primary to-secondary rounded-2xl blur opacity-25 group-hover:opacity-40 transition-opacity"></div>
        <div class="relative bg-base-100 rounded-2xl p-8 border border-base-300 h-full flex flex-col">
          <div class="flex gap-1 mb-6">
            <% 5.times do %>
              <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-warning fill-warning" viewBox="0 0 24 24">
                <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
